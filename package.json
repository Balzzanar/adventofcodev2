{
  "name": "Advent-of-code-v2",
  "version": "0.0.1",
  "description": "Solutions to Advent of code",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "buildstart": "npm build && npm start",
    "clean": "rm -rf dist",
    "lint": "tslint -p .",
    "precommit": "npm lint",
    "start": "node dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:Balzzanar/adventofcodev2.git"
  },
  "engine": {
    "node": ">8.0.33"
  },
  "files": [
    "dist"
  ],
  "author": "Balzzanar",
  "license": "Apache-2.0",
  "directories": {
    "lib": "dist"
  },
  "dependencies": {
  },
  "devDependencies": {
    "@types/node": "^8.0.33",
    "@types/source-map-support": "^0.4.0",
    "doubleagent": "^1.1.0",
    "nyc": "^11.7.1",
    "ts-node": "^6.0.1",
    "tslint": "^5.9.1",
    "tslint-config-shopify": "^3.0.1",
    "typescript": "^2.8.3"
  },
  "nyc": {
    "include": [
      "src/**/*.ts",
      "src/**/*.tsx"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "lcov",
      "html"
    ],
    "sourceMap": true,
    "instrument": true
  }
}
